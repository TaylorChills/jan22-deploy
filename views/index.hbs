
<div>
<table class="table">
<thead>
    <tr>
      <th class="rank" >#</th>
      <th class="fixed">Name</th>
      <th class="fixed">Price</th>
      <th class="fixed">24Hr Change</th>
      <th class="links" >info</th>
      <th class="links">add</th>
    </tr>
  </thead>
</table>

{{#each data}}
<table class="table" >
  <tbody>
   <tr>
    <td class="rank">{{this.market_cap_rank}}</td>
    <td class="fixed">{{this.name}}</td>
    <td class="fixed">{{this.current_price}}</td>
    <td class="fixed">{{this.price_change_percentage_24h}}</td>
    <td class="links"><a href="/crypto-info/{{this.id}}">info</a></td>
    <td class="links"><a href="/crypto-add/{{this.id}}">add</a></td>
</tr>
    </tbody>

    
</table>
{{/each}}

</div>
