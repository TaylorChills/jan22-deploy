{{#each data}}
<table>
  <tr>
    <th>Rank</th>
    <th>Name</th>
    <th>Price</th>
    <th>24Hr Change</th>
    <th></th>
  </tr>
  
</table>
<a href="/crypto-info/{{this.id}}">info</a>


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">24Hr Change</th>
    </tr>
  </thead>
  <tbody>
   <tr>
    <td>{{this.market_cap_rank}}</td>
    <td>{{this.name}}</td>
    <td>{{this.current_price}}</td>
    <td>{{this.price_change_percentage_24h}}</td>

  </tr>
    
  </tbody>
</table>

{{/each}}
