{{#each data}}
<table>
  <tr>
    <th>Rank</th>
    <th>Name</th>
    <th>Price</th>
    <th>24Hr Change</th>
    <th></th>
  </tr>
  <tr>
    <td>{{this.market_cap_rank}}</td>
    <td>{{this.name}}</td>
    <td>{{this.current_price}}</td>
    <td>{{this.price_change_percentage_24h}}</td>

  </tr>
</table>
<a href="/crypto-info/{{this.id}}">info</a>
{{/each}}



